pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
function _init()
	px = 4
	py = 24
	dx = 0
	dy = 0
	grav = 0.2
end 

function _draw()
	cls()
	map(0,0,0,0,16,16)
	spr(index,px,py,1,1,direction)
end

function _update()
	dx = 0
	if (btn(0)) then
		direction = true
		if checkcolleft() then
			dx = 0
		else
		 dx = -1
		end
	end
	
	if (btn(1)) then
		direction = false
		if checkcolright() then
	 	dx = 0
		else
		 dx = 1
		end
	end
	
	if grounded() then
	dy = 0
	py = flr(flr(py)/8)*8
	else dy += grav
	end
	
	if jump() then
	 py -= 2.5
	 index = 161
	else
	 index = 160
 end
	
	px += dx
	py += dy
	
end

function grounded()
	v = mget(flr(px+4)/8, flr(py)/8+1)
	return fget(v, 0)
end

function checkcolright()
	rgt = mget(flr(px)/8+1, flr(py+5)/8)
	return fget(rgt, 0)
end

function checkcolleft()
	lft = mget(flr(px-1)/8, flr(py+5)/8)
	return fget(lft, 0)
end

function jump()
	if (btn(4) or btn(2)) return true
end
